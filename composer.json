{
    "name": "rndwiga/migratory",
    "type": "library",
    "description": "Raphael migration package",
    "keywords": ["laravel", "rndwiga",  "migrations"],
    "homepage": "https://github.com/tyondoltd/",
    "authors": [
        {
            "name": "Raphael Ndwiga",
            "email": "raphndwi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.3.6"
    },
    "autoload": {
        "psr-4": {
            "Tyondo\\Migratory\\": "src/"
        },
        "files": [

        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tyondo\\Migratory\\MigratoryServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
